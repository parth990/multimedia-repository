public class ArrayOfNames {
	
	public static void main (String [] args)
	{
		
	int age [] = new int[];
	String names[] = new String[];
	
	names = new String [] {"Judy", "Ammar", "Joe", "Nallini", "Markus"};
		
	age = new int [] { 10, 22, 34, 42, 55}
	
	for( int i =0; i<5; i++)
	{
  	if (names [i] .equals ("Judy"))
    System.out.println(names[i]+ " is " + age[i] + " years old");
    }
     	
	}
		
	}
